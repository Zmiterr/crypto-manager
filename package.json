{
  "name": "crypto-manager",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:ts": "yarn ts & yarn dev",
    "ts": "tsc --noEmit --incremental",
    "ts:watch": "yarn ts --watch",
    "build": "next build",
    "start": "next start",
    "pretty": "prettier --write ./**/*.ts",
    "lint": "next lint .",
    "lint:fix": "next lint . --fix",
    "test": "jest",
    "e2e": "start-server-and-test dev http://localhost:3000 \"cypress open --e2e\"",
    "e2e:headless": "start-server-and-test dev http://localhost:3000 \"cypress run --e2e\"",
    "component": "cypress open --component",
    "component:headless": "cypress run --component",
    "e2e:synpress": "env-cmd -f .env npx synpress run -cf synpress.json --config supportFile='cypress/support/index.js'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/cache": "11.11.0",
    "@emotion/react": "11.11.1",
    "@emotion/server": "11.11.0",
    "@emotion/styled": "11.11.0",
    "@ethersproject/bignumber": "^5.7.0",
    "@metamask/detect-provider": "^2.0.0",
    "@mui/base": "5.0.0-beta.26",
    "@mui/material": "5.14.20",
    "@redux-devtools/extension": "^3.3.0",
    "@reduxjs/toolkit": "^2.0.1",
    "@synthetixio/synpress": "^3.7.2-beta.9",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "axios": "^1.6.2",
    "clsx": "^2.0.0",
    "date-fns": "2.30.0",
    "env-cmd": "^10.1.0",
    "ethers": "^6.9.0",
    "formik": "^2.4.5",
    "jest-environment-jsdom": "^29.7.0",
    "lodash": "4.17.21",
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.5.5",
    "react-qr-code": "^2.0.12",
    "react-redux": "^9.0.3",
    "redux": "5.0.0",
    "sass": "^1.69.5",
    "start-server-and-test": "^2.0.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3",
    "typograf": "^7.3.0",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@types/lodash": "^4.14.202",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "babel-eslint": "10.1.0",
    "babel-plugin-root-import": "6.6.0",
    "cypress": "^13.6.1",
    "eslint": "^8.55.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-tss-unused-classes": "^1.0.2",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint:fix",
      "eslint --cache --fix"
    ]
  },
  "baseUrl": "http://localhost:3000",
  "volta": {
    "node": "18.17.1",
    "yarn": "1.19.2"
  }
}